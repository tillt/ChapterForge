PROJECT_NAME           = "ChapterForge"
PROJECT_NUMBER         = "@CHAPTERFORGE_VERSION@"
OUTPUT_DIRECTORY       = "@CMAKE_CURRENT_BINARY_DIR@/docs"
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
GENERATE_XML           = YES
XML_OUTPUT             = xml
RECURSIVE              = YES
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = NO
EXTRACT_LOCAL_CLASSES  = YES
QUIET                  = YES
WARN_IF_UNDOCUMENTED   = NO
INLINE_SIMPLE_STRUCTS  = YES
MARKDOWN_SUPPORT       = YES
USE_MDFILE_AS_MAINPAGE = @CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen_mainpage.md
FULL_SIDEBAR           = YES
DISABLE_INDEX          = NO
GENERATE_TREEVIEW      = YES
FILE_PATTERNS          = *.hpp \
                         *.md
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/include \
                         @CMAKE_CURRENT_SOURCE_DIR@/docs/doxygen_mainpage.md \
                         @CMAKE_CURRENT_SOURCE_DIR@/README.md
EXCLUDE                = @CMAKE_CURRENT_SOURCE_DIR@/include/nlohmann
